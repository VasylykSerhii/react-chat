State('')\n  let history = useHistory();\n\n  const register = useCallback(\n    (e: React.MouseEvent<HTMLAnchorElement>) => {\n      auth.createUserWithEmailAndPassword(email, password)\n    }, [email, password],\n  )\n\n  console.log(auth);\n\n  const signIn = useCallback(\n    (e: React.MouseEvent<HTMLAnchorElement>) => {\n      auth.signInWithEmailAndPassword(email, password)\n        .then(res => {\n          dispatch(getUseCreation(res))\n          history.push(\"/\")\n        })\n    }, [email, password],\n  )\n  const signInWithGoogle = () => {\n    const provider = new firebase.auth.GoogleAuthProvider();\n    auth.signInWithPopup(provider)\n      .then((result) => {\n        dispatch(getUseCreation(result))\n        history.push(\"/\");\n      }).catch((error) => {\n        // Handle Errors here.\n        var errorCode = error.code;\n        var errorMessage = error.message;\n        // The email of the user's account used.\n        var email = error.email;\n        // The firebase.auth.AuthCredential type that was used.\n        var credential = error.credential;\n        // ...\n      });\n  }\n\n  return (\n    <Wraper>\n      <Img src={manImg} alt=\"avatar\" />\n      <Form>\n        <InputWrap>\n          <FontAwesomeIcon icon={faUser} size=\"lg\" color=\"#222831\" />\n          <Input\n            placeholder=\"Email\"\n            onChange={e => setEmail(e.target.value)}\n          />\n        </InputWrap>\n        <InputWrap>\n          <FontAwesomeIcon icon={faLock} size=\"lg\" color=\"#222831\" />\n          <Input\n            placeholder=\"Password\"\n            onChange={e => setPassword(e.target.value)}\n\n          />\n        </InputWrap>\n        <Button\n          onClick={isRegister ? register : signIn}\n          type=\"button\"\n        >{isRegister ? \"Sign Up\" : \"Sign In\"}</Button>\n      </Form>\n      <Link>Recover password</Link>\n      <Link onClick={() => setIsRegister(!isRegister)}>\n        {isRegister ? \"Sign In\" : \"Sign Up\"}\n      </Link>\n      <SocialWrap>\n        <FontAwesomeIcon\n          icon={faGooglePlus}\n          size=\"2x\"\n          color=\"#eeeeee\"\n          onClick={signInWithGoogle}\n        />\n        <FontAwesomeIcon icon={faFacebook} size=\"2x\" color=\"#eeeeee\" />\n      </SocialWrap>\n    </Wraper>\n  )\n}\n\nexport default LogIn\n","/Users/admin/Documents/job/test-tasks/react-chat/src/components/Message/index.tsx",["134","135"],"import React, { useRef, useEffect } from \"react\"\nimport { Wrapper } from './style.component'\nimport { IChatMessages } from 'pages/Chat'\n\nimport { auth } from \"../../utils/firebase\";\nimport { MessageWrap, Avatar, MessageText, Name, Text } from './style.component'\n\ntype Props = {\n  message: IChatMessages,\n  key?: string\n}\n\nconst Message = ({ message, key }: Props): JSX.Element => {\n  return (\n    <Wrapper>\n      <MessageWrap myMessage={auth?.currentUser?.uid === message.uid}>\n        <Avatar src={message.photoURL} />\n        <MessageText myMessage={auth?.currentUser?.uid === message.uid}>\n          <Name>{message.fullName}</Name>\n          <Text>{message.text}</Text>\n        </MessageText>\n      </MessageWrap>\n    </Wrapper >\n  )\n}\n\nexport default Message\n","/Users/admin/Documents/job/test-tasks/react-chat/src/components/Layout/index.tsx",[],"/Users/admin/Documents/job/test-tasks/react-chat/src/components/LogInForm/style-component.ts",[],"/Users/admin/Documents/job/test-tasks/react-chat/src/components/Layout/style.component.ts",[],"/Users/admin/Documents/job/test-tasks/react-chat/src/components/Message/style.component.tsx",[],"/Users/admin/Documents/job/test-tasks/react-chat/src/components/Menu/index.tsx",[],"/Users/admin/Documents/job/test-tasks/react-chat/src/components/Menu/style.component.ts",[],{"ruleId":"136","replacedBy":"137"},{"ruleId":"138","replacedBy":"139"},{"ruleId":"140","severity":1,"message":"141","line":1,"column":21,"nodeType":"142","messageId":"143","endLine":1,"endColumn":30},